{
  "name": "Quickly query price stocks",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab","tabs", "scripting"],
  "host_permissions": ["https://api.khochangchang.com"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/key16.png",
      "32": "/images/key32.png",
      "48": "/images/key48.png",
      "128": "/images/key128.png"
    }
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "css": ["web_style.css"],
        "js":["web_script.js"]
    }],
  "icons": {
    "16": "/images/key16.png",
    "32": "/images/key32.png",
    "48": "/images/key48.png",
    "128": "/images/key128.png"
  },
  "options_page": "options.html"
}
